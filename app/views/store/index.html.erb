
<h2>All Categories:</h2>
<% @categories.each do |category| %>
  <span> |   <%= link_to category.title, category_path(category) %> | </span>
<% end %><br><br>

<h3>All Items:</h3>

<% @items.each do |item| %>
  <p><%= item.title %> | <%= number_to_currency(item.price) %> | <%= button_to 'Add to Cart', line_items_path(item_id: item) %></p>
<% end %>